{
    "name": "@nodescript/fetch-undici",
    "version": "1.6.2",
    "description": "Unified fetch implementation using Node.js + Undici",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": "./out/main/index.js"
    },
    "main": "out/main/index.js",
    "types": "out/main/index.d.ts",
    "files": [
        "out/main/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "tsc -b -w",
        "compile": "tsc -b",
        "lint": "eslint --cache .",
        "test": "NODE_ENV=test mocha",
        "version": "npm run compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/nodescript/fetch-undici .git"
    },
    "keywords": [],
    "author": "Boris Okunskiy",
    "license": "ISC",
    "devDependencies": {
        "@nodescript/eslint-config": "^2.1.0",
        "@types/mocha": "^8.2.3",
        "@types/node": "^16.18.87",
        "eslint": "^9.25.1",
        "mocha": "^11.1.0",
        "pre-commit": "^1.0.10",
        "typescript": "^5.5.2"
    },
    "dependencies": {
        "@nodescript/core": "^7.25.2",
        "@nodescript/metrics": "^1.7.1",
        "lru-cache": "^11.1.0",
        "undici": "^6.19.2"
    }
}
